page {
  /* background-color: #f1f1f1; */
  padding-top: 1rpx;
}

.clss-v {
  height: 90rpx;
  width: 100%;
  border-bottom: 1rpx solid #f1f1f1;
  position: fixed;
  background-color: white;
  margin-top: -1rpx;
}

.clss_btn {
  width: 33%;
  height: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #333333;
  display: inline-block;
}

.clss-tit {
  height: 87rpx;
  line-height: 87rpx;
}

.lis {
  width: 110rpx;
  height: 6rpx;
  background-color: #1296db;
  border-radius: 3rpx;
  margin: 0 auto;
}

.lisv-back-v {
  margin-top: 90rpx;
}

.list-v {
  width: 100%;
  height: 233rpx;
  /* margin-bottom: 20rpx; */
  border-bottom: 20rpx solid #f1f1f1;
  background-color: white;
}

.list-cell{
  height: 100%;
  margin: 0 33rpx;
}

.header-v {
  width: 100%;
  height: 70rpx;
  border-bottom: 1rpx solid #f1f1f1;
}

.header-s {
  width: 50%;
  height: 100%;
  line-height: 70rpx;
  display: inline-block;
  font-size: 20rpx;
  color: #999999;
}

.con-v {
  width: 100%;
  height: 50rpx;
}


.con-tit {
  width: 50%;
  height: 100%;
  line-height: 50rpx;
  display: inline-block;
  font-size: 24rpx;
  color: #333333;
}

.colorg{
  color: #1296db;
}

