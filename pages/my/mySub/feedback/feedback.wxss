/* pages/my/mySub/feedback/feedback.wxss */

.tit {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #999999
}

.feedback-con {
  width: 100%;
  height: 200rpx;
  background-color: #f1f1f1;
  border-radius: 10rpx;
  overflow: hidden;
  margin-bottom: 25rpx;
}

.feedback {
  width: 94%;
  height: 136rpx;
  font-size: 28rpx;
  margin-top: 32rpx;
  margin-left: 20rpx;
  margin-bottom: 32rpx;
  margin-right: 20rpx;
  color: #333333;
}

.image-con {
  width: 100%;
  height: 216rpx;
  margin: 25rpx 0rpx;
}

.image-v {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  border: 1rpx solid #e5e5e5;
  display: inline-block;
  margin-right: 25rpx;
}

.img {
  width: 100%;
  height: 100%;
}

.tel {
  height: 110rpx;
  line-height: 110rpx;
  border-radius: 10rpx;
  background-color: #f1f1f1;
  text-align: left;
  margin: 0rpx 20rpx;
}

.submission {
   height: 96rpx;
   width: 594rpx;
   margin: 0 auto;
   margin-top: 108rpx;
}

.double-v {
  position: absolute;
  right: 0rpx;
  z-index: 999;
}

.hr-img-v {
  width: 40rpx;
  height: 40rpx;
}
