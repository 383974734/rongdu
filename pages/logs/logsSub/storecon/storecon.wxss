/* pages/logs/logsSub/storecon/storecon.wxss */

.back-v {
  margin: 32rpx;
  height: 738rpx;
  border-radius: 10rpx;
}

.img {
  width: 100%;
  height: 380rpx;
  border-radius: 10rpx;
}

.cell-v {
  margin: 0rpx 32rpx;
  height: 110rpx;
  border-bottom: 1rpx solid #e5e5e5;
}

.cell-tit {
  height: 110rpx;
  line-height: 110rpx;
  font-size: 34rpx;
  color: #333333;
  float: left;
}

.cell-state {
  width: 110rpx;
  height: 100%;
  line-height: 110rpx;
  font-size: 28rpx;
  float: right;
  text-align: right;
  color: #f1464e;
}

.spot {
  height: 16rpx;
  width: 16rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  margin-top: 42rpx;
  background-color: red;
  float: right;
}

.cell-state-tit {
  width: 60rpx;
  height: 100%;
  line-height: 110rpx;
  float: right;
}

.add-v {
  width: 460rpx;
  height: 80rpx;
  margin-top: 10rpx;
  border-right: 1rpx solid #e5e5e5;
  float: left;
}

.add-img {
  width: 110rpx;
  height: 33rpx;
}

.time-img {
  width: 170rpx;
  height: 35rpx;
}


.add-tit {
  height: 50rpx;
  margin-right: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
  color: #999999;
  word-break:keep-all;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis; 
}

.add-icon {
  float: right;
  width: 160rpx;
}

.ic-mimg {
  width: 48rpx;
  height: 48rpx;
  margin-right: 32rpx;
  margin-top: 24rpx;
  border-radius: 50%;
  float: right;
}

.icon-tit {
  height: 42rpx;
  width: 125rpx;
  line-height: 42rpx;
  text-align: center;
  font-size: 18rpx;
  color: #999999;
  float: right;
}



.time-v {
  width: 100%;
  height: 80rpx;
  margin-top: 10rpx;
}