/* pages/logs/logsSub/help/help.wxss */

.con-v {
  margin: 0rpx;
  margin-bottom: 330rpx;
}

.header-tit {
  height: 110rpx;
  line-height: 110rpx;
  font-size: 48rpx;
  color: #333333;
  margin-left: 32rpx;
}



/* 分类父试图容器 */
.listTow-bs{
  overflow: hidden;
  background-color: #ffffff;
  margin-bottom: 25rpx;
}

.listTow-b {
  overflow: hidden;
  background-color: #ffffff;
}

.lis-v {
  width: 100%;
  height: 1rpx;
  margin-right: 0rpx;
  margin-left: 32rpx;
  background-color: #e5e5e5;
  float: left;
}

/* 分类文字 */
.listTow-tit {
  float: left;
  font-size: 28rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #999999;
  margin-left: 32rpx;
}

.listTow-push {
  float: right;
  width: 50rpx;
  height: 50rpx;
  margin-top: 25rpx;
  margin-right: 25rpx;
}

.listTow-x {
  width: 100%;
  height: 5rpx;
  background-color: #f7f7f7;
}

.listTow-xs {
  width: 100%;
  height: 25rpx;
  background-color: #f7f7f7;
}


.sub-v {
  margin: 0rpx 0rpx 0rpx 32rpx;
  background-color: #f1f1f1;
  padding: 16rpx 0rpx 16rpx 16rpx;
}

.sub-tit{
  width: 97%;
  line-height: 30rpx;
  font-size: 24rpx;
  color: #999999;
}



.bottom-v {
  position:fixed;
  display:flex;
  width:100%;
  height: 320rpx;
  bottom: 0rpx;
  justify-content:center;
  border-top: 1rpx solid #f1f1f1;
  background-color: white;
}

.help-tit {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin-left: 32rpx;
  font-size: 24rpx;
  color: #999999;
}


.btn {
  width: 600rpx;
  height: 96rpx;
  position: absolute;
  left: 50%;
  margin-left: -300rpx;
  top: 100rpx;
}


.btn-img {
  width: 100%;
  height: 100%;
}